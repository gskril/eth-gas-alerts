{
  "name": "eth-gas-alerts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && printf '_worker.js\\n' > dist/.assetsignore",
    "preview": "astro preview",
    "astro": "astro",
    "deploy:worker": "bun run build && wrangler deploy",
    "seed-history": "bun --env-file=.dev.vars run scripts/seed-history.ts",
    "seed-history:prod": "SEED_REMOTE=1 bun --env-file=.dev.vars run scripts/seed-history.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro}\" \"*.mjs\" \"scripts/**/*.ts\""
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.0",
    "@astrojs/react": "^4.2.1",
    "@astrojs/tailwind": "^6.0.2",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource/dm-mono": "^5.2.7",
    "@tanstack/react-query": "^5.90.21",
    "astro": "^5.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "uplot": "^1.6.31",
    "viem": "^2.23.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.7.0",
    "wrangler": "4"
  }
}
